type Maybe a = Nothing | Just a;

Bool isNothing((Maybe a) m) {
	match (m) {
		case (Nothing) return True;
		case (Just a) return False;
	}
	return True;
}

Void main () {
	(Maybe Int) n = new Nothing;
	(Maybe Int) o = new Just 1;
	
	print (isNothing(n));
	print (isNothing(o));
	
	match (o) {
		case (Nothing) print(4);
		case (Just i) print(i);
	}
}
